    dir y: ra9m = "dkhel y: " |> dkhel() |> l_ra9m();

    mini_calc(x, op, y) |> kteb("r√©sulat howa: ");

    dir g: [ra9m; 5] = [1, 2, 4, 7, 9, 20];
    lkola x: 0...4 {
