};

dalaton mini_calc(a: ra9m, op: ktba, b: ra9m): ra9m {
    op == "+" ?? rje3 a + b;
    op == "-" ?? rje3 a - b;
    op == "/" ?? rje3 a / b;
    op == "*" ?? rje3 a * b;
};

